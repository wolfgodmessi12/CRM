<%
	# app/views/user_contact_form_marketplaces/js/_show.js.erb
	# 
	# Example:
	# 	render partial: "user_contact_form_marketplaces/js/show", locals: { cards: Array }
	# 
	# Required Instance Variables:
	# 	cards[1]
	# 		@user_contact_form: (UserContactForm)
	# 	cards[2]
	# 		@user_contact_form: (UserContactForm)
	# 
	# Required Local Variables:
	#   cards: (Array)
	# 
  cards = [local_assigns.dig(:cards) || []].flatten
-%>
// Buy modal
<% if cards.include?('show') %>
	$("#dash_modal").modal("show");
  $('#dash_modal_title').html('Get a QuickPage');
	$('#dash_modal_body').html('<%= j render partial: 'user_contact_form_marketplaces/show' -%>');
	$('#dash_modal_footer').html('<%= j render partial: 'user_contact_form_marketplaces/show_footer' -%>');
<% end %>

// Edit modal
<% if cards.include?('edit') %>
  $('#user_contact_form_marketplace_modal').modal({ backdrop: 'static' });
  $('#user_contact_form_marketplace_modal').modal('show');
  $('#user_contact_form_marketplace_modal_title').html('<i class="fa fa-funnel-dollar mr-2"></i>Campaign Marketplace Information');
  $('#user_contact_form_marketplace_modal_body').html('<%= j render partial: 'user_contact_form_marketplaces/edit' -%>');
<% end %>

Looper.init();
