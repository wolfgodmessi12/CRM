<%
	# app/views/user_contact_forms/js/_show.js.erb
	# 
	# Example:
	# 	render partial: "user_contact_forms/js/show", locals: { cards: (Array) }
	# 
	# Required Instance Variables:
	# 	cards[1]
	# 		@user_contact_forms: (UserContactForms)
	# 	cards[2]
	# 		@user_contact_form:  (UserContactForm)
	# 
	# Required Local Variables:
	#   cards: (Array)
	# 
  cards = [local_assigns.dig(:cards) || []].flatten
-%>
<% if cards.include?('index') %>
	$('#user_contact_forms_body').html('<%= j render partial: 'user_contact_forms/index' -%>');
<% end %>
<% if cards.include?('import') %>
  $('#dash_modal').modal('show');
  $('#dash_modal_title').html('<i class="fa fa-cloud-download-alt"></i> Import QuickPage');
  $('#dash_modal_body').html('<%= j render partial: 'user_contact_forms/import' -%>');
  $('#dash_modal_footer').html('');
<% end %>
<% if cards.include?('user_contact_forms_dropdown') %>
  $('#select_user_contact_forms_dropdown').html('<%= j render partial: 'user_contact_forms/select_user_contact_forms_dropdown' -%>');
<% end %>
<% if cards.include?('hide_dash_modal') %>
  $('#dash_modal').modal('hide');
<% end %>

Looper.init();
