<%
   # app/views/clients/users/index.turbo_stream.erb
   #
   # Example:
   #   render partial: 'clients/users/index'
   #
   # Instance Variables:
   #   @client: (Client)
   #
   # Local Variables:
   #   none
   #
   return unless @client.is_a?(Client)

   presenter = ClientPresenter.new(client: @client)
-%>
<%= turbo_stream.update 'client_page_section' do %>
  <%= render partial: 'clients/users/index', locals: { client_presenter: presenter } %>
<% end %>

<%= turbo_stream.bootstrap_init %>
