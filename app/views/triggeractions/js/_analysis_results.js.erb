<%
  # /app/views/triggeractions/js/_analysis_results.js.erb
  #
  # Example:
  #   render partial: 'triggeractions/js/analysis_results', locals: { triggeraction: Triggeraction }
  #
  # Instance Variables:
  #   none
  #
  # Local Variables:
  #   (req) triggeraction: Triggeraction
  #
  return unless local_assigns.dig(:triggeraction).is_a?(Triggeraction)
-%>
$('#triggeraction_analysis_results_<%= triggeraction.id -%>').html('<%= j render partial: 'triggeractions/analyze_icon', locals: { triggeraction: } -%>');
$('#triggeraction_analysis_results_<%= triggeraction.id -%>').tooltip('dispose');
$('#triggeraction_analysis_results_<%= triggeraction.id -%>').tooltip({
  html: true,
  placement: 'right',
  container: 'body',
  title: '<div class="text-left"><%= j render partial: 'triggeractions/show/analysis_info', locals: { triggeraction: } -%></div>',
  template: '<div class="tooltip" role="tooltip"><div class="arrow"></div><div class="tooltip-inner tooltip-big"></div></div>'
});
$('#triggeraction_analyzed_<%= triggeraction.id -%>').html('<%= j render partial: 'triggeractions/analyze_icon_with_name', locals: { triggeraction: } -%>');
$('#triggeraction_analysis_results_help').html('<%= j render partial: 'triggeractions/show/analysis_info', locals: { triggeraction: } -%>');
